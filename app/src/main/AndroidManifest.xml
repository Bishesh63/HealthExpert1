<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="net.bishesh.HealthExpert">
    <uses-permission android:name="android.permission.VIBRATE"/>


    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name="net.bishesh.HealthExpert.ui.ResetPasswordActivity"></activity>
        <activity android:name="net.bishesh.HealthExpert.ui.HomeActivity"></activity>
        <activity android:name="net.bishesh.HealthExpert.ui.RegisterActivity"></activity>
        <activity android:name="net.bishesh.HealthExpert.ui.BmiActivity"></activity>
        <activity android:name="net.bishesh.HealthExpert.ui.ForumActivity"></activity>
        <activity android:name="net.bishesh.HealthExpert.ui.MedalertActivity"></activity>
        <activity android:name="net.bishesh.HealthExpert.ui.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>